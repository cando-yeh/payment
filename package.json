		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.18",
		"@testing-library/svelte": "^5.3.1",
		"@types/dotenv": "^6.1.1",
		"@vitest/coverage-v8": "^4.0.18",
		"bits-ui": "^2.15.5",
		"clsx": "^2.1.1",
		"jsdom": "^28.0.0",
		"mode-watcher": "^1.1.0",
