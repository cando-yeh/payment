{
	"name": "-new",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:run": "vitest run",
		"test:e2e": "playwright test",
		"test:preflight": "bash scripts/test-preflight.sh",
		"test:e2e:stable": "playwright test --workers=2",
  "test:all:stable": "npm run test:preflight && npm run test:run && npm run test:e2e:stable",
  "test:cleanup": "node scripts/cleanup-test-data.mjs"
	},
	"devDependencies": {
		"@internationalized/date": "^3.11.0",
